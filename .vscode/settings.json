  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#4c5372",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["string", "punctuation.definition.string"],
        "settings": {
          "foreground": "#8bd49d"
        }
      },
      {
        "scope": ["constant.numeric", "constant.language", "constant.other"],
        "settings": {
          "foreground": "#e9b093"
        }
      },
      {
        "scope": ["keyword", "storage.modifier", "storage.type"],
        "settings": {
          "foreground": "#b68acb"
        }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#5cb3c1"
        }
      },
      {
        "scope": ["variable", "variable.parameter", "variable.other"],
        "settings": {
          "foreground": "#D5D7E5"
        }
      },
      {
        "scope": ["entity.name.type", "support.type", "storage.class"],
        "settings": {
          "foreground": "#e9b093"
        }
      },
      {
        "scope": ["entity.name.tag", "support.class"],
        "settings": {
          "foreground": "#e96590"
        }
      },
      {
        "scope": ["punctuation", "meta.brace"],
        "settings": {
          "foreground": "#8a8ea8"
        }
      }
    ]
  }
}
